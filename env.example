# StashPlayaVR API Environment Configuration
# Copy this file to .env and update with your actual values

# StashApp Configuration
STASHAPP_URL=http://localhost:9999
STASHAPP_GRAPHQL_URL=http://localhost:9999/graphql
STASHAPP_API_KEY=your-stash-api-key-here

# JWT Configuration
JWT_SECRET=your-jwt-secret-key-here
JWT_ISSUER=StashPlayaVR-API
JWT_AUDIENCE=StashPlayaVR-Client

# Application Configuration
APP_HOST=0.0.0.0
APP_PORT=8890
APP_SHOW_ALL_ACTORS=false
APP_SORT_POPULARITY_BY_RATING=false

# User Configuration (format: username=password)
USERS_ADMIN=password123
USERS_USER=userpass

# Logging Configuration
LOGGING_LOGLEVEL_DEFAULT=Information
LOGGING_LOGLEVEL_MICROSOFT_ASPNETCORE=Warning

# Environment
ASPNETCORE_ENVIRONMENT=Production
ASPNETCORE_URLS=http://+:8890